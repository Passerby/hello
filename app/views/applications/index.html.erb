<div class="page-header">
  <h1>首页广告位</h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>用户</th>
      <th>简历</th>
      <th>职位</th>
      <th>时间</th>
    </tr>
  </thead>
  <tbody>
    <% @applications.each do |app| %>
      <tr>
        <td><%= app.user_id %></td>
        <td><%= app.resume_id %></td>
        <td><%= app.job_id %></td>
        <td><%= app.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_application_path,
            :class => 'btn btn-primary hidden' %>
