<p id="notice"><%= notice %></p>

<h1>Listing Admin Setting Industries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_setting_industries.each do |admin_setting_industry| %>
      <tr>
        <td><%= admin_setting_industry.name %></td>
        <td><%= link_to 'Show', admin_setting_industry %></td>
        <td><%= link_to 'Edit', edit_admin_setting_industry_path(admin_setting_industry) %></td>
        <td><%= link_to 'Destroy', admin_setting_industry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Industry', new_admin_setting_industry_path %>
