<p id="notice"><%= notice %></p>

<h1>Listing Admin Setting Job Titles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Industry</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_setting_job_titles.each do |admin_setting_job_title| %>
      <tr>
        <td><%= admin_setting_job_title.name %></td>
        <td>
          <%= Admin::Setting::Industry.find(admin_setting_job_title).name %>
        </td>
        <td><%= link_to 'Show', admin_setting_job_title %></td>
        <td><%= link_to 'Edit', edit_admin_setting_job_title_path(admin_setting_job_title) %></td>
        <td><%= link_to 'Destroy', admin_setting_job_title, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job title', new_admin_setting_job_title_path %>
