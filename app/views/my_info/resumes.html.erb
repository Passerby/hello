<%= render 'link_header' %>

<div class="my-info-content-page-header">
  <%= t('my_resumes')  %>
</div>

<div class="my-info-content">
  <% current_user.resumes.each do |r| %>
    <div class="clearfix">
      <span class="col-sm-9"><%= t('preview') %>: <%= link_to r.name, "#{Rails.application.config.resumePath}#{r.path}", target: "_blank" %></span>
      <span class="col-sm-3"><%= r.created_at.strftime("%Y %b %d")  %></span>
    </div>
  <% end %>
</div>