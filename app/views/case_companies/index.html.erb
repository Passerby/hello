<p id="notice"><%= notice %></p>

<h1>Listing Case Companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Img</th>
      <th>Requirement</th>
      <th>Usage</th>
      <th>Effect</th>
      <th>Lang</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @case_companies.each do |case_company| %>
      <tr>
        <td><%= case_company.name %></td>
        <td><%= case_company.img %></td>
        <td><%= case_company.requirement %></td>
        <td><%= case_company.usage %></td>
        <td><%= case_company.effect %></td>
        <td><%= case_company.lang %></td>
        <td><%= case_company.active %></td>
        <td><%= link_to 'Show', case_company %></td>
        <td><%= link_to 'Edit', edit_case_company_path(case_company) %></td>
        <td><%= link_to 'Destroy', case_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Case company', new_case_company_path %>
